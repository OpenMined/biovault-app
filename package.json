{
	"name": "biovault-app",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"engines": {
		"node": ">=23.6.1",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"start": "expo start --dev-client",
		"reset-project": "node ./scripts/reset-project.js",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"lint": "expo lint",
		"prebuild": "npx expo prebuild --platform all --clean",
		"build-dev": "npx eas build --profile development --platform ios --local --clear-cache",
		"build-dev-android": "npx eas build --profile development --platform android --local --clear-cache",
		"build-prod": "npx eas build --profile production --platform ios --local --clear-cache",
		"build-preview-android": "npx eas build --profile preview --platform android --local --clear-cache",
		"deploy-preview-android": "npx eas build --profile preview --platform android --clear-cache",
		"deploy-dev": "npx eas build --profile development --platform ios --clear-cache",
		"deploy-dev-all": "npx eas build --profile development --platform all --clear-cache",
		"deploy-dev-android": "npx eas build --profile development --platform android --clear-cache",
		"deploy-prod": "npx eas build --profile production --platform ios --auto-submit --clear-cache",
		"deploy-prod-android": "npx eas build --profile production --platform --auto-submit android --clear-cache",
		"cargo-android": "node ./scripts/cargo-android.mts",
		"cargo-ios": "node ./scripts/cargo-ios.mts",
		"device": "npx expo run:ios --device"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^2.2.0",
		"@react-navigation/native": "^7.1.6",
		"expo": "54.0.8",
		"expo-asset": "~12.0.8",
		"expo-constants": "~18.0.9",
		"expo-device": "~8.0.8",
		"expo-document-picker": "~14.0.7",
		"expo-file-system": "~19.0.11",
		"expo-font": "~14.0.7",
		"expo-linking": "~8.0.8",
		"expo-router": "~6.0.6",
		"expo-secure-store": "~15.0.7",
		"expo-splash-screen": "~31.0.10",
		"expo-sqlite": "~16.0.8",
		"expo-status-bar": "~3.0.8",
		"expo-web-browser": "^15.0.7",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.4",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-keyboard-controller": "1.18.5",
		"react-native-reanimated": "~4.1.0",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-web": "^0.21.0",
		"react-native-webview": "13.15.0",
		"react-native-worklets": "0.5.1"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@types/react": "~19.1.10",
		"eas-cli": "^16.18.1",
		"eslint": "^9.25.0",
		"eslint-config-expo": "~10.0.0",
		"expo-dev-client": "~6.0.12",
		"typescript": "~5.9.2"
	},
	"private": true
}
