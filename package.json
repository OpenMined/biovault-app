{
	"name": "biovault-app",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"reset-project": "node ./scripts/reset-project.js",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"lint": "expo lint",
		"prebuild": "npx expo prebuild --platform ios --clean",
