{
	"cli": {
		"version": ">= 16.18.1",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "preview"
			},
			"autoIncrement": true
		},
		"production": {
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {}
	}
}
